{
  "manifest_version": 2,

  "name": "Address Bar Completion",
  "description": "Adds tab completion to the address bar.",
  "version": "1.0",
  "author": "Adam Alton <adamalton@gmail.com>",

  "omnibox": { "keyword" : "abc" },
  "background": {
    "scripts": ["abc.js"]
  },
  "permissions": [
    "https://api3.codebasehq.com/"
  ],
  "browser_action": {
    "default_icon": "codebase.png",
    "default_popup": "settings.html"
  }

}
